{
  "name": "file-upload-refreshed",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "tsx server.ts",
    "build:next": "next build",
    "build:server": "npx tsc --project tsconfig.server.json",
    "build": "npm run build:next && npm run build:server",
    "start": "NODE_ENV=production node --expose-gc dist/server.js",
    "postinstall": "prisma generate",
    "lint": "next lint",
    "format": "prettier '**/**/*.{js,ts,jsx,tsx,json}' --write",
    "depcheck": "depcheck --oneline",
    "check": "npm-check -u",
    "generate": "prisma generate",
    "generate:backup": "prisma generate --schema=./db-sync/prisma/schema.prisma",
    "migrate": "prisma migrate dev",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.14.0",
    "@prisma/client": "^5.22.0",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@types/busboy": "^1.5.4",
    "@types/multer": "^1.4.12",
    "@types/node-cron": "^3.0.11",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^10.0.0",
    "busboy": "^1.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "depcheck": "^1.4.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "framer-motion": "^11.11.17",
    "next": "^15.0.3",
    "next-auth": "^4.24.10",
    "next-themes": "^0.4.3",
    "npm-check": "^6.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "react": "^18.3.1",
    "react-dom": "^18",
    "react-dropzone": "^14.3.5",
    "react-hot-toast": "^2.4.1",
    "tailwind-merge": "^2.5.4",
